# ============================================================================
# OpenClaw - Daily Journaling Assistant
# Copy this file to .env and fill in your values
# Usage: ./install.sh --env-file .env
# ============================================================================

# ---- AI Provider ----
AI_PROVIDER=grok
OPENCLAW_PRIMARY_MODEL=grok-3
XAI_API_KEY=
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_GEMINI_API_KEY=
OPENROUTER_API_KEY=

# ---- Channels ----
TELEGRAM_BOT_TOKEN=
DISCORD_BOT_TOKEN=
SLACK_BOT_TOKEN=
SLACK_APP_TOKEN=
WHATSAPP_ENABLED=false

# ---- SMTP ----
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=openclaw@localhost

# ---- Gateway ----
OPENCLAW_GATEWAY_TOKEN=
OPENCLAW_GATEWAY_BIND=lan

# ---- Desktop / VNC ----
ENABLE_VNC=true
VNC_PASSWORD=
DISPLAY_RESOLUTION=1920x1080x24

# ---- Tor ----
TOR_ENABLED=true

# ---- Use Case Specific ----
JOURNAL_PROMPT_TIME=21:00\nJOURNAL_DIR=workspace/journal\nMOOD_TRACKING=true\nWEEKLY_REFLECTION=true

